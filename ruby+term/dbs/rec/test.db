test {
	id		int;
	name	varchar(256);
	price	number(6,2);
};

primary{
	id;
};

path all {
	DBSELECT {
		DECLARE test_all_csr CURSOR FOR
		SELECT * FROM test ORDER BY ID;
	};
};

path key {
	DBSELECT {
		DECLARE test_all_crs CURSOR FOR
		SELECT * FROM test WHERE id = :id;
	};
	DBINSERT {
		INSERT INTO test VALUES(:id,:name,:price);
	};
	DBUPDATE {
		UPDATE test
		SET name 	= :name,
			price 	= :price
		WHERE id = :id;
	};
	DBDELETE {
		DELETE FROM test WHERE id = :id ;
	};
};
